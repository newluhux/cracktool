#!/usr/bin/env expect 

set timeout 5

spawn picocom -b 115200 /dev/ttyACM0

expect "Terminal ready" {
	send "\r"
}

expect "login: " {
	send "$env(username)\r"
	expect "Password: " {
		send "$env(password)\r"
	}
}

expect "# " {
	puts "login success!"
	puts "username: $env(username)"
	puts "password: $env(password)"
}
